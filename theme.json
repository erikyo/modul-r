{
	"$schema": "https://schemas.wp.org/wp/6.3/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"color": {
			"background": true,
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultDuotone": true,
			"defaultGradients": true,
			"defaultPalette": false,
			"gradients": [],
			"palette": [
				{
					"color": "#6f4cad",
					"name": "Primary",
					"slug": "primary"
				},
				{
					"color": "#9b82c6",
					"name": "Primary Light",
					"slug": "primary-light"
				},
				{
					"color": "#4e367a",
					"name": "Primary Dark",
					"slug": "primary-dark"
				},
				{
					"color": "#16bebb",
					"name": "Secondary",
					"slug": "secondary"
				},
				{
					"color": "#5cd2d0",
					"name": "Secondary Light",
					"slug": "secondary-light"
				},
				{
					"color": "#108583",
					"name": "Secondary Dark",
					"slug": "secondary-dark"
				},
				{
					"color": "#110e17",
					"name": "Black",
					"slug": "black"
				},
				{
					"color": "#262428",
					"name": "Gray dark",
					"slug": "gray-dark"
				},
				{
					"color": "#888888",
					"name": "Gray light",
					"slug": "gray"
				},
				{
					"color": "#e3e3e3",
					"name": "Gray light",
					"slug": "gray-light"
				},
				{
					"color": "#f3f3f3",
					"name": "White Smoke",
					"slug": "white-smoke"
				},
				{
					"color": "#ffffff",
					"name": "White",
					"slug": "white"
				},
				{
					"color": "var(--wp--preset--color--black)",
					"name": "Text Color",
					"slug": "text-color"
				},
				{
					"color": "var(--wp--preset--color--white)",
					"name": "Background",
					"slug": "background"
				}
			],
			"text": true
		},
		"custom": {
			"animations": {
				"length": "350ms"
			},
			"line-height": {
				"large": 2.4,
				"medium": 1.7,
				"small": 1.2
			}
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1600px"
		},
		"spacing": {
			"spacingScale": {
				"increment": 2,
				"mediumStep": 1,
				"operator": "*",
				"steps": 7,
				"unit": "rem"
			},
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"dropCap": true,
			"fluid": true,
			"fontFamilies": [
				{
					"fontFamily": "var(--typography--font--title)",
					"name": "Title",
					"slug": "title"
				},
				{
					"fontFamily": "var(--typography--font--default)",
					"name": "Default",
					"slug": "default"
				},
				{
					"fontFamily": "monospace, serif",
					"name": "Monospace",
					"slug": "monospace"
				}
			],
			"fontSizes": [
				{
					"size": "16px",
					"slug": "baseline"
				},
				{
					"name": "Extra small",
					"size": "0.7rem",
					"slug": "extra-small"
				},
				{
					"name": "Small",
					"size": "0.9rem",
					"slug": "small"
				},
				{
					"name": "Default",
					"size": "1rem",
					"slug": "default"
				},
				{
					"name": "Medium",
					"size": "1.4rem",
					"slug": "medium"
				},
				{
					"name": "Large",
					"size": "1.8rem",
					"slug": "large"
				},
				{
					"name": "Extra large",
					"size": "2.6rem",
					"slug": "extra-large"
				},
				{
					"name": "Extra large",
					"size": "4rem",
					"slug": "xx-large"
				},
				{
					"name": "Extra Extra large",
					"size": "7rem",
					"slug": "xxx-large"
				}
			],
			"fontStyle": true,
			"fontWeight": true,
			"letterSpacing": true,
			"textDecoration": true,
			"textTransform": true
		}
	},
	"styles": {
		"spacing": {
			"padding": {
				"right": "var(--wp--preset--spacing--50)",
				"left": "var(--wp--preset--spacing--50)"
			}
		},
		"blocks": {
			"core/button": {
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--background)"
				},
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--default)",
					"fontSize": "var(--wp--preset--font-size--medium)",
					"fontStyle": "normal",
					"fontWeight": "var(--typography--font--default--bold)",
					"letterSpacing": "0.33em",
					"lineHeight": "var(--wp--custom--line-height--small)",
					"textTransform": "uppercase"
				}
			},
			"core/buttons": {
				"spacing": {
					"blockGap": "var(--wp--preset--spacing--60)",
					"margin": {
						"bottom": "var(--wp--preset--spacing--60)",
						"top": "var(--wp--preset--spacing--40)"
					}
				}
			},
			"core/post-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			},
			"core/separator": {
				"color": {
					"background": "var(--wp--preset--color--primary)"
				},
				"spacing": {
					"margin": {
						"bottom": "var(--wp--preset--spacing--60)",
						"top": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"core/site-title": {
				"elements": {
					"link": {
						"color": {
							"text": "var(--wp--preset--color--primary-dark)"
						},
						"typography": {
							"textDecoration": "none"
						}
					}
				}
			}
		},
		"color": {
			"background": "var(--wp--preset--color--background)",
			"text": "var(--wp--preset--color--text-color)"
		},
		"elements": {
			"button": {
				"border": {
					"radius": "0"
				},
				"color": {
					"background": "var(--wp--preset--color--primary)",
					"text": "var(--wp--preset--color--white)"
				},
				"typography": {
					"fontSize": "1.4rem",
					"fontStyle": "normal",
					"fontWeight": "var(--typography--font--default--bold)",
					"lineHeight": "1"
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				},
				"typography": {
					"fontFamily": "var(--typography--font--title)",
					"fontStyle": "normal",
					"fontWeight": "var(--typography--font--title--bold)",
					"lineHeight": "var(--wp--custom--line-height--small)"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--primary)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--primary-dark)"
				},
				"typography": {
					"fontWeight": "var(--typography--font--default--bold)",
					"textDecoration": "none"
				}
			}
		},
		"typography": {
			"fontFamily": "var(--typography--font--default), sans-serif",
			"fontSize": "var(--wp--preset--font-size--baseline)",
			"fontWeight": "var(--typography--font--default--regular)",
			"lineHeight": "var(--wp--custom--line-height--medium)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		},
		{
			"area": "uncategorized",
			"name": "comments",
			"title": "Comments"
		},
		{
			"area": "uncategorized",
			"name": "sidebar",
			"title": "Sidebar"
		}
	],
	"version": 2
}
