{
  "$schema": "https://schemas.wp.org/trunk/theme.json",
  "version": 2,
  "settings": {
	"border": {
	  "radius": false,
	  "color": false,
	  "style": false,
	  "width": false
	},
	"color": {
	  "custom": true,
	  "customDuotone": true,
	  "customGradient": true,
	  "duotone": [],
	  "gradients": [],
	  "link": false,
	  "text": true,
	  "background": true,
	  "defaultGradients": true,
	  "defaultPalette": false
	},
	"layout": {
	  "contentSize": "1000px",
	  "wideSize": "1500px"
	},
	"spacing": {
	  "blockGap": true,
	  "units": [
		"%",
		"px",
		"em",
		"rem",
		"vh",
		"vw"
	  ]
	},
	"typography": {
	  "customFontSize": true,
	  "lineHeight": false,
	  "dropCap": true,
	  "fontStyle": true,
	  "fontWeight": true,
	  "letterSpacing": true,
	  "textDecoration": true,
	  "textTransform": true
	},
	"custom": {
	  "hero": {
		"title": {
		  "font-size": "64px"
		}
	  },
	  "hamburger": {
		"color": "var(--header--text-color)"
	  },
	  "sizes": {
		"entry-title": {
		  "width": "66%"
		}
	  }
	}
  },
  "styles": {
	"color": {
	  "text": "var(--color--text)"
	},
	"spacing": {
	  "blockGap": "2rem"
	}
  }
}
