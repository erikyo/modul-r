{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "customTemplates": [
	{
	  "name": "404",
	  "postTypes": [
		"page"
	  ],
	  "title": "404"
	}
  ],
  "settings": {
	"appearanceTools": true,
	"border": {
	  "color": true,
	  "radius": true,
	  "style": true,
	  "width": true
	},
	"color": {
	  "background": true,
	  "custom": true,
	  "customDuotone": true,
	  "customGradient": true,
	  "defaultDuotone": true,
	  "defaultGradients": true,
	  "defaultPalette": false,
	  "gradients": [],
	  "link": true,
	  "text": true
	},
	"custom": {
	  "animations": {
		"length": "350ms"
	  },
	  "line-height": {
		"large": 2.2,
		"medium": 1.7,
		"small": 1.2
	  },
	  "responsive": {
		"side-margin": "var(--wp--preset--spacing--50)"
	  },
	  "sizes": {
		"entry-title": {
		  "width": "66%"
		}
	  }
	},
	"layout": {
	  "contentSize": "1000px",
	  "wideSize": "1600px"
	},
	"spacing": {
	  "spacingScale": {
		"increment": 2,
		"mediumStep": 1,
		"operator": "*",
		"steps": 7,
		"unit": "rem"
	  },
	  "units": [
		"%",
		"px",
		"em",
		"rem",
		"vh",
		"vw"
	  ]
	},
	"typography": {
	  "customFontSize": true,
	  "dropCap": true,
	  "fontFamilies": [
		{
		  "fontFamily": "var(--typography--font--title)",
		  "name": "Title",
		  "slug": "title"
		},
		{
		  "fontFamily": "var(--typography--font--default)",
		  "name": "Default",
		  "slug": "default"
		},
		{
		  "fontFamily": "monospace, serif",
		  "name": "Monospace",
		  "slug": "monospace"
		}
	  ],
	  "fontSizes": [
		{
		  "size": "16px",
		  "slug": "baseline"
		},
		{
		  "name": "Extra small",
		  "size": "0.7rem",
		  "slug": "extra-small"
		},
		{
		  "name": "Small",
		  "size": "0.9rem",
		  "slug": "small"
		},
		{
		  "name": "Medium",
		  "size": "1rem",
		  "slug": "medium"
		},
		{
		  "name": "Large",
		  "size": "1.4rem",
		  "slug": "large"
		},
		{
		  "name": "Extra large",
		  "size": "2rem",
		  "slug": "extra-large"
		},
		{
		  "name": "Extra large",
		  "size": "2.6rem",
		  "slug": "XX-large"
		},
		{
		  "name": "Extra large",
		  "size": "2.6rem",
		  "slug": "XXX-large"
		}
	  ],
	  "fontStyle": true,
	  "fontWeight": true,
	  "letterSpacing": true,
	  "textDecoration": true,
	  "textTransform": true
	}
  },
  "styles": {
	"blocks": {
	  "core/button": {
		"color": {
		  "background": "var(--wp--preset--color--primary)",
		  "text": "var(--wp--preset--color--white)"
		},
		"typography": {
		  "fontFamily": "var(--wp--preset--font-family--title)",
		  "fontSize": "var(--wp--preset--font-size--medium)"
		}
	  }
	},
	"color": {
	  "background": "#ffffff",
	  "text": "var(--color--text)"
	},
	"elements": {
	  "button": {
		"color": {
		  "background": "#ffffff",
		  "text": "var:preset|color|white"
		},
		"typography": {
		  "fontSize": "1.4rem",
		  "fontStyle": "normal",
		  "fontWeight": "700"
		}
	  },
	  "heading": {
		"color": {
		  "text": "#28293e"
		},
		"typography": {
		  "fontFamily": "var(--typography--font--title)"
		}
	  }
	},
	"spacing": {
	  "blockGap": "2rem"
	},
	"typography": {
	  "fontFamily": "var(--typography--font--default), sans-serif",
	  "fontSize": "var(--wp--preset--font-size--baseline)",
	  "fontWeight": "var(--typography--font--default--regular)",
	  "lineHeight": "var(--wp--custom--line-height--medium)"
	}
  },
  "templateParts": [
	{
	  "area": "header",
	  "name": "header",
	  "title": "Header"
	},
	{
	  "area": "footer",
	  "name": "footer",
	  "title": "Footer"
	},
	{
	  "area": "uncategorized",
	  "name": "comments",
	  "title": "Comments"
	},
	{
	  "area": "uncategorized",
	  "name": "post-meta",
	  "title": "Post Meta"
	}
  ],
  "version": 2
}
