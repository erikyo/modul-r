{
  "$schema": "https://schemas.wp.org/wp/6.1/theme.json",
  "customTemplates": [
	{
	  "name": "blank",
	  "title": "Blank",
	  "postTypes": [
		"page",
		"post"
	  ]
	},
	{
	  "name": "404",
	  "title": "404",
	  "postTypes": [
		"page"
	  ]
	},
	{
	  "name": "header-portrait",
	  "title": "Header Portrait",
	  "postTypes": [
		"page",
		"post"
	  ]
	},
	{
	  "name": "sidebar",
	  "title": "Sidebar",
	  "postTypes": [
		"page",
		"post"
	  ]
	}
  ],
  "settings": {
	"useRootPaddingAwareAlignments": true,
	"appearanceTools": true,
	"border": {
	  "color": true,
	  "radius": true,
	  "style": true,
	  "width": true
	},
	"color": {
	  "background": true,
	  "custom": true,
	  "customDuotone": true,
	  "customGradient": true,
	  "defaultDuotone": true,
	  "defaultGradients": true,
	  "defaultPalette": false,
	  "gradients": [],
	  "text": true
	},
	"custom": {
	  "animations": {
		"length": "350ms"
	  },
	  "line-height": {
		"large": 2.4,
		"medium": 1.7,
		"small": 1.2
	  }
	},
	"layout": {
	  "contentSize": "1000px",
	  "wideSize": "1600px"
	},
	"spacing": {
	  "spacingScale": {
		"increment": 2,
		"mediumStep": 1,
		"operator": "*",
		"steps": 7,
		"unit": "rem"
	  },
	  "units": [
		"%",
		"px",
		"em",
		"rem",
		"vh",
		"vw"
	  ]
	},
	"typography": {
	  "customFontSize": true,
	  "dropCap": true,
	  "fluid": true,
	  "fontFamilies": [
		{
		  "fontFamily": "var(--typography--font--title)",
		  "name": "Title",
		  "slug": "title"
		},
		{
		  "fontFamily": "var(--typography--font--default)",
		  "name": "Default",
		  "slug": "default"
		},
		{
		  "fontFamily": "monospace, serif",
		  "name": "Monospace",
		  "slug": "monospace"
		}
	  ],
	  "fontSizes": [
		{
		  "size": "16px",
		  "slug": "baseline"
		},
		{
		  "name": "Extra small",
		  "size": "0.7rem",
		  "slug": "extra-small"
		},
		{
		  "name": "Small",
		  "size": "0.9rem",
		  "slug": "small"
		},
		{
		  "name": "Medium",
		  "size": "1rem",
		  "slug": "medium"
		},
		{
		  "name": "Large",
		  "size": "1.4rem",
		  "slug": "large"
		},
		{
		  "name": "Extra large",
		  "size": "2rem",
		  "slug": "extra-large"
		},
		{
		  "name": "Extra large",
		  "size": "2.6rem",
		  "slug": "xx-large"
		},
		{
		  "name": "Extra Extra large",
		  "size": "4rem",
		  "slug": "xxx-large"
		}
	  ],
	  "fontStyle": true,
	  "fontWeight": true,
	  "letterSpacing": true,
	  "textDecoration": true,
	  "textTransform": true
	}
  },
  "styles": {
	"blocks": {
	  "core/site-title": {
		"elements": {
		  "link": {
			"color": {
			  "text": "var(--wp--preset--color--primary-dark)"
			},
			":hover": {
			  "color": {
				"text": "var(--wp--preset--color--white)"
			  }
			},
			"typography": {
			  "textDecoration": "none"
			}
		  }
		}
	  },
	  "core/navigation": {
		"typography": {
		  "textDecoration": "none",
		  "fontWeight": "var(--typography--font--default--bold)"
		},
		"elements": {
		  "link": {
			"color": {
			  "text": "var(--wp--preset--color--primary-dark)"
			},
			":hover": {
			  "color": {
				"text": "var(--wp--preset--color--white)"
			  }
			}
		  }
		}
	  },
	  "core/button": {
		"color": {
		  "background": "var(--wp--preset--color--primary)",
		  "text": "var(--wp--preset--color--white)"
		},
		"typography": {
		  "fontFamily": "var:preset|font-family|default",
		  "fontSize": "var(--wp--preset--font-size--medium)",
		  "fontStyle": "normal",
		  "fontWeight": "var(--typography--font--default--bold)",
		  "letterSpacing": "0.33em",
		  "lineHeight": "var(--wp--custom--line-height--small)",
		  "textTransform": "uppercase"
		}
	  },
	  "core/buttons": {
		"spacing": {
		  "blockGap": "var:preset|spacing|60",
		  "margin": {
			"bottom": "var:preset|spacing|60",
			"top": "var:preset|spacing|40"
		  }
		}
	  },
	  "core/navigation": {
		"elements": {
		  "link": {
			":hover": {
			  "color": {
				"text": "var(--wp--preset--color--white)"
			  }
			},
			"color": {
			  "text": "var(--wp--preset--color--primary-dark)"
			}
		  }
		},
		"typography": {
		  "fontWeight": "var(--typography--font--default--bold)",
		  "textDecoration": "none"
		}
	  },
	  "core/paragraph": {
		"spacing": {
		  "padding": {
			"bottom": "var:preset|spacing|50",
			"left": "0",
			"right": "0",
			"top": "0"
		  }
		}
	  },
	  "core/separator": {
		"color": {
		  "background": "var:preset|color|primary"
		},
		"spacing": {
		  "margin": {
			"bottom": "var:preset|spacing|80",
			"top": "var:preset|spacing|80"
		  }
		}
	  }
	},
	"color": {
	  "background": "#ffffff",
	  "text": "var:preset|color|black"
	},
	"elements": {
	  "button": {
		"border": {
		  "radius": "0"
		},
		"color": {
		  "background": "var:preset|color|primary",
		  "text": "var:preset|color|white"
		},
		"typography": {
		  "fontSize": "1.4rem",
		  "fontStyle": "normal",
		  "fontWeight": "var(--typography--font--default--bold)",
		  "lineHeight": "1"
		}
	  },
	  "heading": {
		"color": {
		  "text": "var:preset|color|primary"
		},
		"typography": {
		  "fontFamily": "var(--typography--font--title)",
		  "fontStyle": "normal",
		  "fontWeight": "var(--typography--font--title--bold)",
		  "lineHeight": "var(--wp--custom--line-height--small)"
		}
	  },
	  "link": {
		":hover": {
		  "color": {
			"text": "var:preset|color|primary"
		  },
		  "typography": {
			"textDecoration": "none"
		  }
		},
		"color": {
		  "text": "var:preset|color|primary-dark"
		},
		"typography": {
		  "textDecoration": "none"
		}
	  }
	},
	"spacing": {
	  "blockGap": "var:preset|spacing|40",
	  "padding": {
		"top": "0",
		"bottom": "0",
		"left": "var:preset|spacing|50",
		"right": "var:preset|spacing|50"
	  }
	},
	"typography": {
	  "fontFamily": "var(--typography--font--default), sans-serif",
	  "fontSize": "var(--wp--preset--font-size--baseline)",
	  "fontWeight": "var(--typography--font--default--regular)",
	  "lineHeight": "var(--wp--custom--line-height--medium)"
	}
  },
  "templateParts": [
	{
	  "area": "header",
	  "name": "header",
	  "title": "Header"
	},
	{
	  "area": "header",
	  "name": "header-portrait",
	  "title": "Header Portrait"
	},
	{
	  "area": "footer",
	  "name": "footer",
	  "title": "Footer"
	},
	{
	  "area": "uncategorized",
	  "name": "comments",
	  "title": "Comments"
	},
	{
	  "area": "uncategorized",
	  "name": "post-meta",
	  "title": "Post Meta"
	},
	{
	  "area": "uncategorized",
	  "name": "sidebar",
	  "title": "Sidebar"
	}
  ],
  "version": 2
}
